                <style>
                    #megaStore {
                        max-width: 600px;
                        margin: 3px auto;
                        background: url('../content/web/menu/header.jpg') no-repeat center 0;
                    }

                    #menu1X h2 {
                        font-size: 1em;
                        text-transform: uppercase;
                        padding: 5px 10px;
                    }

                    #template img {
                        margin: 5px 20px 0 0;
                        float: left;
                    }

                    #template {
                        width: 380px;
                    }

                        #template ol {
                            float: left;
                            margin: 0 0 0 30px;
                            padding: 10px 10px 0 10px;
                        }

                        #template:after {
                            content: ".";
                            display: block;
                            height: 0;
                            clear: both;
                            visibility: hidden;
                        }

                        #template .k-button {
                            float: left;
                            clear: left;
                            margin: 5px 0 5px 12px;
                        }
                </style>
                <script>
                                $(document).ready(function () {
