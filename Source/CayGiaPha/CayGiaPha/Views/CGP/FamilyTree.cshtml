
@{
    ViewBag.Title = "FamilyTree";
    Layout = "~/Views/Shared/_Layout_2.cshtml";
}


<input id="TreeID" value="@ViewBag.Id" hidden />
<!-- Main -->
<div id="main">

    <!-- Post -->
    <article class="post">
        <header>
            <div id="example">
                <div id="megaStore">
                    <ul id="menu1X">
                        <li>
                            Báo cáo
                            <ul>
                                <li class="clickMe" onclick="BaoCaoNam(@ViewBag.Id,1)">
                                    Lập báo cáo năm
                                </li>
                            </ul>
                        </li>
                        <li>
                            Thay đổi quy định
                            <ul>
                                <li>
                                    Nghề nghiệp
                                    <ul>
                                        <li id="btnShowAddJob">Thêm</li>
                                        <li id="btnShowUpdateJob">Cập nhật</li>
                                    </ul>
                                </li>
                                <li>
                                    Quê quán
                                    <ul>
                                        <li id="btnShowAddBirthPlace">Thêm</li>
                                        <li id="btnShowUpdateBirthPlace">Cập nhật</li>
                                    </ul>
                                </li>
                                <li>
                                    Nguyên nhân mất
                                    <ul>
                                        <li id="btnShowAddCoD">Thêm</li>
                                        <li id="btnShowUpdateCoD">Cập nhật</li>
                                    </ul>
                                </li>
                                <li>
                                    Địa điểm mai táng
                                    <ul>
                                        <li id="btnShowAddBrP">Thêm</li>
                                        <li id="btnShowUpdateBrP">Cập nhật</li>
                                    </ul>
                                </li>
                            </ul>
                        </li>
                        <li>
                            Ghi nhận
                            <ul>
                                <li id="btnShowUA">
                                    Ghi nhận thành tích
                                </li>
                                <li id="btnShowUD">
                                    Ghi nhận kết thúc
                                </li>
                            </ul>
                        </li>
                    </ul>
                </div>
                <style>
                    #megaStore {
                        max-width: 600px;
                        margin: 3px auto;
                        background: url('../content/web/menu/header.jpg') no-repeat center 0;
                    }

                    #menu1X h2 {
                        font-size: 1em;
                        text-transform: uppercase;
                        padding: 5px 10px;
                    }

                    #template img {
                        margin: 5px 20px 0 0;
                        float: left;
                    }

                    #template {
                        width: 380px;
                    }

                        #template ol {
                            float: left;
                            margin: 0 0 0 30px;
                            padding: 10px 10px 0 10px;
                        }

                        #template:after {
                            content: ".";
                            display: block;
                            height: 0;
                            clear: both;
                            visibility: hidden;
                        }

                        #template .k-button {
                            float: left;
                            clear: left;
                            margin: 5px 0 5px 12px;
                        }
                </style>
                <script>
                                $(document).ready(function () {
                                    $("#menu1X").kendoMenu();
                                });
                </script>
            </div>
        </header>
        <div id="tabstrip">
            <ul>
                <li class="k-state-active">
                    Tab 1
                </li>
                <li>
                    New York
                </li>
            </ul>
            <div>
                <div id="Guid" class="container-fluid">
                    <h1>Danh Sách Thành Viên</h1>
